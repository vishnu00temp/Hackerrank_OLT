vector<int> closestNumbers(vector<int> arr) {
    vector<int>ans;
    sort(arr.begin(),arr.end());
    int d=0,min=INT_MAX,i;
    for(i=0;i<arr.size()-1;i++){
        d=abs(arr[i]-arr[i+1]);
        if(d<min){
            min=d;
        }
    }    
       for(i=0;i<arr.size()-1;i++){
        d=abs(arr[i]-arr[i+1]);
        if(d==min){
            // min=d;
            ans.push_back(arr[i]);
            ans.push_back(arr[i+1]);
        }
        
       }
        return ans; 
